<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-rex-binary">
    <comment>REX Static Rust Executable (ELF)</comment>
    <magic priority="100">
      <match type="string" offset="0" value="\x7FELF">
        <match type="string" offset="8" value="REX" />
      </match>
    </magic>
    <glob pattern="*.Rex"/>
    <glob pattern="*.rex"/>
  </mime-type>
</mime-info>
